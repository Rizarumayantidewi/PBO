package Tugas2;

public class Chairwoman {
    private String nama;

    
    public void setTitelNama(String nama){
        this.nama = nama + " S.T, M.Kom";
    }
    
    public String getTitleNama(){
        return nama;
    }
    
    public int setGaji(int gajiK){
        return gajiK;
    }
    public static void main(String[] args) {

        Chairwoman gaji = new Chairwoman();
        gaji.setTitelNama("Riza Rumayanti Dewi");
        int gajiK = gaji.setGaji(1000000000);
        int lembur = 1000000000;
        int total = gajiK + lembur;
        System.out.println( "Gaji Anda => " + total + " " + gaji.getTitleNama());

        byte nByte = 70;
        short nShort = 500;
        int nInt = 263456;
        long nLong = 1200000;
        char nChar = 'A';
        float nFloat = 190;
        double nDouble = 387;
        boolean nBool = true;
    
        System.out.println("\n");
        System.out.println("Byte = " + nByte);
        System.out.println("Short = " + nShort);
        System.out.println("Integer = " +nInt);
        System.out.println("Long = " +nLong);
        System.out.println("Character = " +nChar);
        System.out.println("Float = " +nFloat);
        System.out.println("Double = " +nDouble);
        System.out.println("Booleran = " +nBool);
    
    }

}